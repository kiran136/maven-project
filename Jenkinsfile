Pipeline{

agent any
  
  stages         
{
   stage('Clone_the_code')
   { steps                              
     {  sh 'echo clone the code'       
     }    
   }


   stage('Execute unit test cases')              
   { steps
      { sh 'echo Executing unit test cases' }
   }
  stage(Building code){
    steps{sh 'echo building code'}
  }
  
}
